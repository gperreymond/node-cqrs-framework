{
  "name": "node-cqrs-framework",
  "version": "0.2.2",
  "description": "Do not use, research only.",
  "main": "index.js",
  "scripts": {
    "setup:deve": "./npm-scripts/setup-deve.sh",
    "test": "./npm-scripts/test.sh",
    "test:standard": "node_modules/.bin/standard --verbose | snazzy",
    "test:unit": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --timeout 5000 --use_strict --recursive test/unit",
    "test:integration": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --timeout 5000 --use_strict --recursive test/integration",
    "test:coverage": "node_modules/.bin/istanbul cover -x 'examples/**/*.*' node_modules/.bin/_mocha -- --opts .mocharc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gperreymond/node-cqrs-framework.git"
  },
  "author": "Gilles Perreymond <gperreymond@gmail.com> (https://github.com/gperreymond)",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gperreymond/node-cqrs-framework/issues"
  },
  "homepage": "https://github.com/gperreymond/node-cqrs-framework#readme",
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "debug": "2.2.0",
    "eraro": "0.4.1",
    "glob": "7.1.1",
    "hoek": "4.1.0",
    "lodash": "4.16.6",
    "rabbot": "1.0.6",
    "rabbus": "0.8.2",
    "uuid": "2.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chance": "1.0.4",
    "codacy-coverage": "2.0.0",
    "istanbul": "0.4.5",
    "joi": "9.2.0",
    "mocha": "3.1.2",
    "mocha-standard": "1.0.0",
    "sinon": "1.17.6",
    "snazzy": "5.0.0",
    "standard": "8.5.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ],
    "ignore": [
      "coverage/**"
    ]
  }
}
